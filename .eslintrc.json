{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-filename-extension": [
            "warn",
            {"extensions": [".js", ".jsx"
                ]
            }
        ],
        "import/prefer-default-export": "off",
        "jsx-quotes": ["error", "prefer-single"
        ]
    }
}
